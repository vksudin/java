public class Main
{
	public static void main(String[] args)
	{
	    int a=18;
        
        int b=6;
        
        System.out.println(a-b);
        
        System.out.println(a+b);
        
        System.out.println(a%b);
        
        System.out.println(a*b);
        
        int sum=a+b;
        
        System.out.println(sum);
		
	}
}
